{
  "name": "react-icon-loader",
  "version": "0.6.0",
  "description": "load svg icons as react components through SVGO",
  "main": "lib/loader.js",
  "typings": "lib/loader.d.ts",
  "bin": {
    "react-icon-ts": "./lib/cli.js"
  },
  "scripts": {
    "build": "rimraf lib && tsc -p tsconfig.prod.json && chmod +x lib/cli.js",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --coverage",
    "prepublishOnly": "npm run lint && npm run build && npm test"
  },
  "author": {
    "name": "Victor Kotseruba",
    "url": "https://github.com/barbuza"
  },
  "homepage": "https://github.com/barbuza/react-icon-loader",
  "repository": {
    "type": "git",
    "url": "https://github.com/barbuza/react-icon-loader.git"
  },
  "engines": {
    "node": ">= 8.0.0"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/glob": "^5.0.30",
    "@types/jest": "^20.0.2",
    "@types/lodash": "^4.14.67",
    "@types/node": "^8.0.5",
    "@types/react": "^15.0.33",
    "@types/react-dom": "^15.5.1",
    "@types/webpack": "^3.0.1",
    "@types/xml-parser": "^1.2.28",
    "glob": "^7.0.5",
    "jest": "^20.0.4",
    "material-design-icons": "^3.0.1",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "rimraf": "^2.5.3",
    "ts-jest": "^20.0.6",
    "ts-node": "^3.1.0",
    "tslint": "^5.4.3",
    "typescript": "^2.3.0"
  },
  "dependencies": {
    "javascript-stringify": "^1.6.0",
    "lodash": "4.x",
    "object-assign": "4.x",
    "svgo": "^0.7.2",
    "xml-parser": "1.2.x"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "mapCoverage": true,
    "testRegex": "\\.spec\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "files": [
    "lib"
  ]
}
